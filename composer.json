{
    "name": "webfuelagency/user-authentication",
    "description": "Generate a default starter kit for Laravel user authentication API",
    "type": "library",
    "homepage": "https://github.com/richard-galolo/user-authentication",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Galolo",
            "email": "richard@webfuelagency.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WebFuelAgency\\UserAuthentication\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebFuelAgency\\UserAuthentication\\UserAuthenticationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
